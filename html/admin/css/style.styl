	/******************************************************************************/

	/* List-based Tree */

	div.reflex-tree {
		border	: 0.1em solid #333;
	}

	div.reflex-tree * {
		padding	: 0px;
		margin	: 0px;
	}

	div.reflex-tree ul {
		list-style-type	: none;
	}

	div.reflex-tree ul.reflex-tree-header > li,
	div.reflex-tree ul.reflex-tree-footer > li,
	ul.reflex-tree-node-columns > li.reflex-tree-node-column {
		display			: inline-block;
		overflow		: hidden;
		white-space		: nowrap;
	}

	ul.reflex-tree-node-columns > li.reflex-tree-node-column:nth-child(n+2) {
		border-left			: 1px dotted #3f3f3f;
		-moz-box-sizing		: border-box;
		box-sizing	: border-box;
	}

	div.reflex-tree ul.reflex-tree-header {
		border-top		: 1px solid #3f3f3f;
		border-bottom	: 1px solid #111;

		font-weight			: bold;
		color				: #eee;
		background-image	: url(../img/template/background.png);
	}

	div.reflex-tree ul.reflex-tree-header > li > span,
	div.reflex-tree ul.reflex-tree-footer > li > span {
		padding-left	: 0.25em;
	}

	div.reflex-tree ul.reflex-tree-header > li:nth-child(n+2) > span,
	div.reflex-tree ul.reflex-tree-footer > li:nth-child(n+2) > span {
		border-left	: 1px solid #3f3f3f;
	}

	ul.reflex-tree-body > li.reflex-tree-node > ul.reflex-tree-node-columns {
		/* Root Node */
		display	: none;
	}

	li.reflex-tree-node-column > * {
		vertical-align	: top;
	}

	li.reflex-tree-node-label > div {
		display	: inline-block;
	}

	div.reflex-tree-node-children {
		overflow-y	: hidden;
		position	: relative;
	}

	div.reflex-tree-node-children > ul {
		width	: 100%;
	}

	span.reflex-tree-node-expand {
		/* Using padding instead of width/height because padding will override the span collapse */
		padding-top		: 16px;
		padding-left	: 16px;

		margin-right	: 0.25em;
	}

	span.reflex-tree-node-expandable {
		display				: inline-block;
		background-image	: url("../img/template/tree_closed.png");
	}

	li.reflex-tree-node.reflex-tree-node-expanded > ul.reflex-tree-node-columns > li.reflex-tree-node-label > span.reflex-tree-node-expandable {
		background-image	: url("../img/template/tree_open.png");
	}

	span.reflex-tree-node-icon img {
		vertical-align	: middle;
		text-align		: center;

		max-width		: 16px;
		max-height		: 16px;

		margin-right	: 0.25em;
	}

	span.reflex-tree-node-checkable-checkbox > input[type="checkbox"] {
		margin-top		: 0.15em;
		margin-right	: 0.25em;
	}

