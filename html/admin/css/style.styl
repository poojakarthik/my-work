	/************************** START - Property List - START **************************/
	/* This is designed to work on Definition Lists, but should work anywhere (extra classes required for non-DLs) */
	/*.reflex-propertylist * {
		-moz-box-sizing	: content-box !important;
	}*/

	.reflex-propertylist {
		position	: relative;
		overflow	: hidden;

		box-sizing		: border-box;
		-moz-box-sizing	: border-box;
	}
	/*
	.reflex-propertylist::after {
		content		: '\0020';
		display		: block;
		height		: 0;
		overflow	: hidden;
		clear		: both;
	}*/

	.reflex-propertylist::before {
		/* Draws the key background */
		content		: '\0020';
		position	: absolute;
		top			: 0;
		left		: 0;
		width		: 30%;
		height		: 100%;

		/*border-left		: 1px solid rgba(0, 0, 0, 0.3);*/
		border-right		: 1px solid rgba(0, 0, 0, 0.3);
		background-image	: -moz-linear-gradient(top, rgba(0, 0, 0, 0.1), rgba(0, 0, 0, 0.2));
		background-image	: -webkit-linear-gradient(top, rgba(0, 0, 0, 0.1), rgba(0, 0, 0, 0.2));

		box-sizing		: border-box;
		-moz-box-sizing	: border-box;
	}

	.reflex-propertylist > .reflex-propertylist-key,
	dl.reflex-propertylist > dt {
		/* Key/Term */
		float	: left;
		clear	: both;
		width	: 30%;

		padding			: 0.2em;
		padding-left	: 0.5em;
		padding-right	: 0.5em;
		font-weight		: bold;
		text-shadow		: 0 1px 0 #fff;
		text-align		: right;
		vertical-align	: top;
		margin			: 0;

		box-sizing		: border-box;
		-moz-box-sizing	: border-box;
	}

	.reflex-propertylist > .reflex-propertylist-value,
	dl.reflex-propertylist > dd {
		/* Value */
		margin	: 0;
		width	: 70%;
		float	: right;
		padding	: 0.2em;
		vertical-align	: top;

		box-sizing		: border-box;
		-moz-box-sizing	: border-box;
	}

	.reflex-propertylist > .reflex-propertylist-key > *,
	dl.reflex-propertylist > dt > * {
		vertical-align	: top;
	}
	.reflex-propertylist > .reflex-propertylist-value > *,
	dl.reflex-propertylist > dd > * {
		vertical-align	: top;
	}

