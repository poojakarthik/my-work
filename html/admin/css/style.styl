	/******************************* CUSTOM CONTROLS ******************************/

	/** Tab/Tab Group Control Styling */

	div.tab-group {
		position	: relative;
	}

	div.tab-group div.tab-page-container {
		border		: none;
		border-top	: 1px solid #000;
	}

	div.tab-group div.tab-page {
		padding				: 0.5em;
		background-color	: #f5f5f5;
	}

	div.tab-group.embedded div.tab-page-container {
		border				: 1px solid #000;
	}

	div.tab-group div.tab-row {
		background-color	: #888;
	}

	div.tab-group.embedded div.tab-row {
		background-color	: inherit;
	}

	div.tab-group div.tab {
		/*float							: left;*/
		display							: inline-block;
		position						: relative;
		top								: 1px;
		margin-left						: 0.25em;
		border							: 1px solid #000;
		background-color				: #ddd;
		padding							: 0.25em;
		border-radius : 0.5em 0.5em 0 0;

		-moz-user-select				: none;
		-webkit-user-select				: none;
	}

	div.tab-group div.tab img.icon {
		vertical-align	: text-bottom;
		margin-right	: 0.25em;
	}

	div.tab-group div.tab:hover {
		background-color				: #fff;
		cursor							: pointer;
	}

	div.tab-group div.tab.selected {
		background-color	: #f5f5f5;
		border-bottom		: 1px solid #f5f5f5;
		font-weight			: bold;
		cursor				: default;
	}
	/******************************************************************************/

	/********************************* INPUT TABLE ********************************/
	table.input {
		width			: 99%;
		margin-left		: auto;
		margin-right	: auto;
		border			: 0.1em solid #1C1C1C;
		border-spacing	: 0;
		font-size		: inherit;
	}

	table.input thead,
	table.input tfoot {
		background-color	: #202020;
		color				: #eee;
		text-align			: left;
	}

	table.input tfoot {
		text-align	: right;
	}

	table.input tbody tr td input[type="text"],
	table.input tbody tr td input[type="number"],
	table.input tbody tr td input[type="password"],
	table.input tbody tr td textarea,
	table.input tbody tr td select {
		width			: 98%;
	}

	table.input tbody tr td input[type="text"].date-formatted {
		width			: 10em;
	}

	table.input tbody tr td img[onclick]:hover {
		cursor			: pointer;
	}

	table.input tfoot a,
	table.input tfoot a:visited,
	table.input tfoot a:link {
		color	: #ffde05;
	}

	table.input tbody tr th {
		text-align			: right;
		padding-right		: 0.5em;
		width				: 10em;
		vertical-align		: top;
	}

	table.input tbody tr td {
		background-color	: #ddd;
		color				: #333;
		padding-left		: 0.5em;
		vertical-align		: top;
	}

	table.input tbody tr td .hide,
	div .hide {
		display				: none;
	}

	table.input tbody tr td div input,
	table.input tbody tr td div select,
	table.input tbody tr td div textarea {
		vertical-align	: top;
	}

	table.input tbody tr td input[type="checkbox"] {
		margin-right	: 0.25em;
		vertical-align	: middle;
	}

	table.input tbody tr td div.mandatory input,
	table.input tbody tr td div.mandatory select,
	table.input tbody tr td div.mandatory textarea {
		background	: #fff;
		color		: #111;
		border		: .1em solid #da8;
	}

	table.input tbody tr td div.invalid input,
	table.input tbody tr td div.invalid select,
	table.input tbody tr td div.invalid textarea {
		background	: #ffecf0;
		color		: #111;
		border		: .1em solid #d30;
	}

	table.input tbody tr td div.valid input,
	table.input tbody tr td div.valid select,
	table.input tbody tr td div.valid textarea {
		background	: #ecfff0;
		color		: #111;
		border		: .1em solid #3d0;
	}

	table.input tbody tr.alt td {
		background-color	: #fff;
	}
	/******************************************************************************/

	/************************* SELECT LIST (and GROUPS) ***************************/
	div.select-list {
		background-color	: #fff;
		border				: 1px solid #000;
		overflow-y			: scroll;

		/* Default Height */
		height				: 10em;
	}

	table.select-list {
		border-collapse	: collapse;
		width			: 100%;
	}

	table.select-list tr:nth-child(even) {
		background-color	: #eee;
	}

	table.select-list tr:hover td {
		background-color	: #b5ccdf;
	}

	table.select-list tr td.icon {
		width	: 0.1em;
		cursor	: pointer;
	}

	div.select-list-group {
		column-count			: 2;
		-moz-column-count		: 2;
		-webkit-column-count	: 2;

		column-gap				: 0.5em;
		-moz-column-gap			: 0.5em;
		-webkit-column-gap		: 0.5em;
	}

	div.select-list-group h4 {
		padding	: 0;
		margin	: 0;
	}
	/******************************************************************************/

	/********************************* TREE GRID **********************************/

	/*** Standard Styling ***/
	div.tree-grid {
		background-color	: #fff;
		border				: 1px solid #000;
		overflow-y			: scroll;

		/* Default Height */
		height				: 15em;

		position			: relative;
	}

	table.tree-grid {
		border-collapse	: collapse;
		width			: 100%;
	}
	/*
	table.tree-grid tr:nth-child(even) {
		background-color	: #eee;
	}
	*/
	table.tree-grid tr:hover td {
		background-color	: #b5ccdf;
	}

	table.tree-grid tr td {
		vertical-align	: top;
	}

	table.tree-grid tr td.icon {
		width			: 0.1em;
	}

	table.tree-grid tr td.tree img {
		width			: 16px;
		min-width		: 16px;
		max-width		: 16px;
		height			: 16px;
		min-height		: 16px;
		max-height		: 16px;
		vertical-align	: top;

		-moz-user-select	: none;
		-webkit-user-select	: none;
	}

	table.tree-grid tr td.tree span.label {
		padding-left	: 0.25em;
	}

	table.tree-grid tr td.tree img.icon {
		/*width		: 16px;*/
		/*min-width	: 16px;*/
		max-width	: 16px;
		/*height		: 16px;*/
		/*min-height	: 16px;*/
		max-height	: 16px;

		vertical-align	: top;
	}

	table.tree-grid tr td.tree img.clickable {
		cursor	: pointer;
	}

	table.tree-grid tr td.row-select {
		width	: 0.1em;
	}

	table.tree-grid tr td.row-select input[type="checkbox"] {
		display	: none;
	}

	table.tree-grid tr td.row-select img {
		-moz-user-select	: none;
		-webkit-user-select	: none;
		cursor				: pointer;
	}

	/*** Additional Styling ***/
	table.tree-grid.permissions tr td {
		font-style	: italic;
		opacity		: 0.5;
	}

	table.tree-grid.permissions tr.selectable td {
		font-style	: normal;
		opacity		: 1;
	}

	div.tree-grid div.loading {
		display				: table;
		background-color	: rgba(150, 150, 150, 0.5);
		height				: 100%;
		width				: 100%;

		position			: absolute;
		top					: 0px;
		left				: 0px;
	}

	div.tree-grid div.loading div {
		display			: table-cell;
		vertical-align	: middle;
		text-align		: center;
		font-style		: italic;
	}

	div.tree-grid div.loading div span {
		border				: 1px solid #000000;
		background-color	: #ffffff;

		display				: inline-block;
		padding				: 0.25em;
	}

	div.tree-grid div.loading img {
		width			: 16px;
		height			: 16px;
		vertical-align	: middle;
	}
	/******************************************************************************/

	/******************************** DATE PICKER *********************************/

	div.reflex-datepicker {
		border				: 0.1em solid #BF8E5F;
		background-color	: #FFF7EF;

		padding		: 0.2em;

		font-size	: 0.8em;

		font-family	: "Trebuchet MS", Verdana, Tahoma, Arial, sans-serif;

		border-top-left-radius : 0.5em;
		border-bottom-left-radius : 0.5em;
		border-top-right-radius : 0.5em;
		border-bottom-right-radius : 0.5em;
	}

	div.reflex-datepicker div.header {

	}

	div.reflex-datepicker div.footer {
		border-top	: 0.1em solid #BF8E5F;
		padding		: 0.25em;

		font-size	: 1.1em;
		font-weight	: bolder;
	}

	div.reflex-datepicker div.footer div.reflex-datepicker-datetime,
	div.reflex-datepicker div.footer div.reflex-datepicker-datetime div.reflex-datepicker-datetime-date,
	div.reflex-datepicker div.footer div.reflex-datepicker-datetime div.reflex-datepicker-datetime-time {
		display	: inline-block;
	}
	div.reflex-datepicker div.footer div.reflex-datepicker-datetime div.reflex-datepicker-datetime-time {
		margin-left	: 0.25em;
	}

	div.reflex-datepicker div.footer div.reflex-datepicker-datetime-time span {
		margin-left		: 0.25em;
		margin-right	: 0.25em;
	}

	div.reflex-datepicker div.content {

	}

	div.reflex-datepicker div.content div.clear {
		clear	: both;
	}

	div.reflex-datepicker div.content div.month {
		/*border	: 1px solid #333;*/
		display			: inline-block;
		vertical-align	: top;
	}

	div.reflex-datepicker div.content div.month caption {
		font-size	: 1.2em;
		font-weight	: bold;

		color		: #fff;

		background-image	: url('../../admin/img/template/wave.png');
		background-color	: #D7812A;
		background-position	: 0% 45%;

		height		: 2em;
		line-height	: 2em;

		vertical-align	: middle;

		border-top		: 0.1em solid #DFA66F;
		border-bottom	: 0.1em solid #8F5823;
		border-left		: 0.1em solid #DFA66F;
		border-right	: 0.1em solid #8F5823;
	}

	div.reflex-datepicker div.content div.month tr.days th {
		font-weight		: bold;
	}

	div.reflex-datepicker div.content div.month thead {
		border-bottom	: 0.1em solid #BF8E5F;
	}

	div.reflex-datepicker div.content div div.month:first-child caption {
		border-top-left-radius : 0.5em;
		border-bottom-left-radius : 0.5em;
	}

	div.reflex-datepicker div.content div div.month:last-child caption {
		border-top-right-radius : 0.5em;
		border-bottom-right-radius : 0.5em;
	}

	div.reflex-datepicker div.content div.month caption span {
		vertical-align	: middle;
		padding			: 0.25em;
	}

	div.reflex-datepicker div.content div.month td {

		min-width	: 2em;
		max-width	: 2em;
		width		: 2em;

		min-height	: 2em;
		max-height	: 2em;
		height		: 2em;

		padding-right	: 0.25em;
	}

	div.reflex-datepicker div.content div.month td.day {
		border	: 0.1em solid #BF8E5F;

		background-color	: #EFE0D1;
		background-image	: url('../../admin/img/template/wave.png');
		background-repeat	: repeat-x;
		background-position	: 50%;

		text-align		: right;
		vertical-align	: middle;
	}

	div.reflex-datepicker div.content div.month td.day.selectable:hover {
		border	: 0.1em solid #BF6A17;

		background-color	: #FFF7EF;

		color		: #BF6A17;
		cursor		: pointer;
	}

	div.reflex-datepicker div.content div.time-picker {
		border-top	: 0.1em solid #BF8E5F;
		/*padding		: 0.25em;*/
		padding		: 2em;

		font-size	: 1.1em;
		font-weight	: bolder;
	}

	/* Day Mutator Classes */
	div.reflex-datepicker div.content div.month td.day.reflex-datepicker-day-today span {
		color			: #BF6A17;
		border-bottom	: 0.1em dotted #BF6A17;
	}

	div.reflex-datepicker div.content div.month td.day.reflex-datepicker-day-selected {
		color				: #fff;
		font-weight			: bolder;

		background-color	: #CF8D4D;
	}

	/******************************************************************************/

	/*********************************** SLIDER ***********************************/

	div.reflex-slider {
		-moz-user-select	:	none;
		-webkit-user-select	:	none;
	}

	div.reflex-slider-rail,
	div.reflex-slider-rail-handle {
		-moz-border-radius-topright			: 0.25em;
		-moz-border-radius-bottomright		: 0.25em;
		-moz-border-radius-topleft			: 0.25em;
		-moz-border-radius-bottomleft		: 0.25em;
		border-top-right-radius		: 0.25em;
		border-bottom-right-radius	: 0.25em;
		border-top-left-radius		: 0.25em;
		border-bottom-left-radius	: 0.25em;
	}

	div.reflex-slider-rail {
		position	: relative;

		border		: 0.1em solid #DFA66F;

		min-height	: 0.5em;
		max-height	: 0.5em;
		height		: 0.5em;
	}

	div.reflex-slider-rail-label {
		position	: absolute;
		top			: -1.5em;

		border				: 0.1em solid #BF6A17;
		background-color	: #fff;
	}

	div.reflex-slider-rail-range {
		position	: absolute;

		min-height	: 100%;
		max-height	: 100%;
		height		: 100%;

		background-color	: #CF8D4D;
		background-image	: url('../../admin/img/template/wave.png');
	}

	div.reflex-slider-rail-handle {
		position	: absolute;
		top			: -0.25em;
		left		: 0%;			/* default position -- overridden element style */

		border		: 0.1em solid #BF6A17;

		background-image	: url('../../admin/img/template/wave.png');
		background-color	: #EFE0D1;

		min-width	: 0.5em;
		max-width	: 0.5em;
		width		: 0.5em;

		min-height	: 1em;
		max-height	: 1em;
		height		: 1em;

		margin-left	: -0.25em;

		cursor		: default;
	}

	div.reflex-slider-rail-handle:hover {
		border-color		: #BF6A17;
		background-color	: #FFF7EF;
	}

	div.reflex-slider-rail-marker {
		margin-left	: -50%;
		position	: absolute;
	}

