// Application
//
@import "vendor"
@import "skin"
@import "reset"

/* This css file is included in both Framework 2 and 3 pages.
	*
	* All styling of new functionality (Framework 3 or 2) should have its
	* styling defined in this file
	*
	* Any styling required for popups or the specific content of a popup, should
	* go here because most popups can be used on top of framework 2 or 3 pages.
	*
	* Framework 3 page styling that adversly affects Framework 2 styling, should be defined in reflex.css as that file is only included in Framework 3 pages
	* Framework 2 page styling that adversly affects Framework 3 styling, should be defined in default.css as that file is only included in Framework 2 pages
	*/

@css {
	div.clearing {
		clear	: both;
	}

	a,
	.pointer {
		cursor : pointer;
	}


	.flex-page-dashboard-container {
		overflow: visible !important;
	}


	/******************************************************************************/
	/* FW Windows */
	/******************************************************************************/
	.fw-layer-window {
		border: 1px solid #000;
		background: transparent;
		position: absolute;
		box-shadow: 0 0 20px #000;
		border-radius: 5px 5px 1px 1px;
		-vnd-animation-duration .1s
		-vnd-animation-name zoom
	}
	@-moz-keyframes zoom {
		from {
			-vnd-transform: scale(0);
		} to {
			-vnd-transform: scale(1);
		}
	}
	@-webkit-keyframes zoom {
		from {
			-vnd-transform: scale(0);
		} to {
			-vnd-transform: scale(1);
		}
	}
	.fw-layer-window .fw-popup-content {
		padding: .5em;
	}
	.fw-layer-window.-closable .fw-popup-header-close {
		display: block;
	}

	/******************************************************************************/
	/* FW Popup */
	/******************************************************************************/
	.fw-layer {
		position: fixed;
		top: 0em;
		left: 0em;
		width: 100%;
		height: 100%;
		z-index: 45;
	}
	.fw-layer-underlay {
		position: absolute;
		top: 0;
		left: 0;
		width: 100%;
		height: 100%;
		background-color: #000000;
		filter: alpha(opacity=25)
		-moz-opacity: .25;
		opacity: .25;
		z-index: 50;
	}
	.fw-layer-window {
		margin: auto;
		border: 0.1em solid rgba(0, 0, 0, 0.40);
		background-color: #EFEFEF;
		z-index: 50;
		position: absolute;
		border-radius: 4px;
		box-shadow: 0 0 40px black;
		overflow: hidden;
	}
	.fw-popup-header * {
		vertical-align: middle;
	}
	.fw-popup-header {
		height: auto;
		padding: 3px;
		/*background: -webkit-linear-gradient(top, rgba(0, 0, 0, 0.10) 0%, rgba(0, 0, 0, 0.20) 100%);*/
		background: linear-gradient(to bottom, rgba(0, 0, 0, .05), rgba(0, 0, 0, .15));
		box-shadow: 0 -8px 8px 8px black;
		-moz-user-select: none;
		-webkit-user-select: none;
	}
	.fw-popup-header-title {
		color: #333;
		text-shadow: 0 1px 0 rgba(255, 255, 255, 0.50);
		display: inline-block;
		font-size: 1em;
		padding: 0;
		margin: 0;	width: 79%;
		font-weight: bold;
		overflow: hidden;
		white-space: nowrap;
		text-overflow: ellipsis;
		text-indent: 0.5em;
	}
	.fw-popup-header-close {
		float: right;
		background-image: url('/admin/img/template/close.png') !important;
		width: 16px;
		height: 16px;
		margin: 0 !important;
		padding: 0 !important;
		border: none !important;
		background-color: transparent !important;
	}
	.fw-popup-alert-buttons {
		text-align: center;
		padding: 0.5em;
	}
	.fw-popup-alert {
		width: 300px;
	}


